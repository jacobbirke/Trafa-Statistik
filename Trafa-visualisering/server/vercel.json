{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js",
        "methods": ["GET", "POST", "OPTIONS"],
              "headers": {
        "Access-Control-Allow-Origin": "*"}
      }
    ]
  }